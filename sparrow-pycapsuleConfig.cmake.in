# sparrow-pycapsule cmake module
# This module sets the following variables in your project::
#
#   sparrow-pycapsule _FOUND - true if sparrow found on the system
#   sparrow-pycapsule _INCLUDE_DIRS - the directory containing sparrow headers
#   sparrow-pycapsule _LIBRARY - empty

@PACKAGE_INIT@

include(CMakeFindDependencyMacro)

if(NOT TARGET sparrow::sparrow-pycapsule )
    include("${CMAKE_CURRENT_LIST_DIR}/@PROJECT_NAME@Targets.cmake")
    get_target_property(@PROJECT_NAME@_INCLUDE_DIRS sparrow::sparrow-pycapsule  INTERFACE_INCLUDE_DIRECTORIES)
    get_target_property(@PROJECT_NAME@_LIBRARY sparrow::sparrow-pycapsule  LOCATION)
endif()
